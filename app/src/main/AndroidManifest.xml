            android:screenOrientation="portrait" />
        <activity
            android:name=".login.VerifyCodeLogin.VerifyCodeLoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".webview.WebviewNormalActivity"
            android:screenOrientation="portrait" />
        <!--<activity-->
            <!--android:name=".register.RegisterFrament"-->
