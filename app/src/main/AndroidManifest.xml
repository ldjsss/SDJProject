            <!--android:screenOrientation="portrait" />-->
        <activity
            android:name=".mine.InputNameActivity"
            android:screenOrientation="portrait"
            android:theme="@style/transparent_activity"
            android:windowSoftInputMode="stateAlwaysVisible" />
        <activity
            android:name=".mine.msg.MsgActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".mine.qrcode.QRCodeActivity"
